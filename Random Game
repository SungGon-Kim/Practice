import random

num = random.randint(1,20)
cnt = 0
print(num)

while True:
    cnt += 1
    usr = int(input('입력 : '))
    if usr == num:
        print('정답입니다!')
        break
    else:
        print("오답입니다.")
        print("*"*40)
        
    if cnt == 10:
        print('5회 초과, 정답은 %d 입니다'%num)
        break
